{% extends 'base.html.twig' %}
{% block stylesheets %}
{{ parent() }}

{% endblock %}

{% block title %}Home{% endblock %}

{% block body %}

{% block nav %}
{% include "browser/browser.html.twig" %}
{% endblock nav %}
    <div class="container-fluid col-2 text-center mt-4">
        {{ form_start(form) }}
        <div class="form-group mt-3">
            {{ form_row(form.firstname, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            {{ form_row(form.lastname, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group mt-3">
            {{ form_row(form.email, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            {{ form_row(form.password.first, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            {{ form_row(form.password.second, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            {{ form_row(form.create, {'attr': {'class': 'form-control btn btn-primary'}}) }}
        </div>
        {{ form_end(form) }}
    </div>
{% endblock %}