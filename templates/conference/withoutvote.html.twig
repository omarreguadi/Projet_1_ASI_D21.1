{% extends 'base.html.twig' %}
{% block body %}
    <div class="container mt-3 mb-3">
        <table class="striped">
            <thead>
            <tr>
                <th>Titre</th>
                <th>Description</th>
                <th>Date de cr√©ation</th>
                <th>Moyenne</th>
            </tr>
            </thead>
            <tbody>
            {% for i in vote %}
                <tr>
                    <td><a href="{{ path('conferenceId', {'id': i.id}) }}">{{ i.title }}</a></td>
                    <td>{{ i.description }}</td>
                    <td>{{ i.createdAt|date("d/m/Y") }}</td>
                    <td>{{ i.scoreAvg|round(1,'floor') }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}
